{
  "name": "wyzie-lib",
  "version": "2.1.2",
  "icon": "https://i.postimg.cc/L5ppKYC5/cclogo.png",
  "license": "MIT",
  "keywords": [
    "wyzie-subs",
    "wyzie subs",
    "subtitle",
    "search",
    "library",
    "typescript",
    "vite",
    "fast",
    "scraper",
    "subtitle scraper",
    "open subtitles"
  ],
  "type": "module",
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "files": [
    "./lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.js"
      },
      "require": {
        "types": "./lib/main.d.ts",
        "default": "./lib/main.umd.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itzcozi/wyzie-lib.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "test": "npx vitest",
    "format": "prettier --log-level warn --write \"{src/**/*.{ts},*.{ts,js,html,css,json,md,xml}}\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^4.5.5",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
