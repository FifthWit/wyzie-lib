{
  "name": "wyzie-lib",
  "version": "1.0.4",
  "keywords": [ "wyzie-subs", "wyzie subs", "subtitle", "search", "library", "typescript", "vite", "fast", "scraper", "subtitle scraper" ],
  "main": "dist/wyzie-lib.umd.js",
  "module": "dist/wyzie-lib.es.js",
  "files": [
    "dist",
    "dist/types"
  ],
  "exports": {
    ".": {
      "import": "./dist/wyzie-lib.es.js",
      "require": "./dist/wyzie-lib.umd.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --log-level warn --write \"src/**/*.{tsx,ts}\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^4.0.0"
  }
}
